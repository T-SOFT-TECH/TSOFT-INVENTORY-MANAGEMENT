{
    "projectId": "tsoftmart-inventory-invoice-system",
    "projectName": "TSOFT-MART INVENTORY/INVOICE SYSTEM",
    "settings": {
        "services": {
            "account": true,
            "avatars": true,
            "databases": true,
            "locale": true,
            "health": true,
            "storage": true,
            "teams": true,
            "users": true,
            "functions": true,
            "graphql": true,
            "messaging": true
        },
        "auth": {
            "methods": {
                "jwt": true,
                "phone": true,
                "invites": true,
                "anonymous": true,
                "email-otp": true,
                "magic-url": true,
                "email-password": true
            },
            "security": {
                "duration": 31536000,
                "limit": 0,
                "sessionsLimit": 10,
                "passwordHistory": 0,
                "passwordDictionary": false,
                "personalDataCheck": false,
                "sessionAlerts": false,
                "mockNumbers": []
            }
        }
    },
    "databases": [
        {
            "$id": "inventory-invoice-db",
            "name": "Inventory-Invoice DB",
            "enabled": true
        }
    ],
    "collections": [
        {
            "$id": "products",
            "$permissions": [
                "read(\"any\")",
                "update(\"any\")",
                "create(\"label:admin\")",
                "read(\"label:admin\")",
                "update(\"label:admin\")",
                "delete(\"label:admin\")"
            ],
            "databaseId": "inventory-invoice-db",
            "name": "Products",
            "enabled": true,
            "documentSecurity": false,
            "attributes": [
                {
                    "key": "name",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "category",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 100,
                    "default": null
                },
                {
                    "key": "description",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 65535,
                    "default": null
                },
                {
                    "key": "price",
                    "type": "double",
                    "required": true,
                    "array": false,
                    "default": null,
                    "min": 0
                },
                {
                    "key": "cost",
                    "type": "double",
                    "required": false,
                    "array": false,
                    "default": null,
                    "min": 0
                },
                {
                    "key": "stockQuantity",
                    "type": "integer",
                    "required": true,
                    "array": false,
                    "default": 0,
                    "min": 0
                },
                {
                    "key": "lowStockThreshold",
                    "type": "integer",
                    "required": true,
                    "array": false,
                    "default": 10,
                    "min": 0
                },
                {
                    "key": "sku",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 50,
                    "default": null
                },
                {
                    "key": "status",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 20,
                    "default": "active",
                    "elements": [
                        "active",
                        "archived"
                    ]
                },
                {
                    "key": "imageUrl",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "totalQuantitySold",
                    "type": "integer",
                    "required": true,
                    "array": false,
                    "default": 0,
                    "min": 0
                },
                {
                    "key": "totalRevenue",
                    "type": "double",
                    "required": true,
                    "array": false,
                    "default": 0,
                    "min": 0
                }
            ],
            "indexes": [
                {
                    "key": "sku",
                    "type": "unique",
                    "attributes": [
                        "sku"
                    ]
                },
                {
                    "key": "category",
                    "type": "key",
                    "attributes": [
                        "category"
                    ]
                },
                {
                    "key": "status",
                    "type": "key",
                    "attributes": [
                        "status"
                    ]
                }
            ]
        },
        {
            "$id": "inventory-invoice-db",
            "databaseId": "inventory-invoice-db",
            "name": "Inventory/Invoice Database",
            "documentSecurity": false,
            "attributes": [],
            "indexes": [],
            "enabled": true
        }
    ],
    "functions": [
        {
            "$id": "adding-attributes",
            "name": "Adding Attributes",
            "runtime": "node-16.0",
            "execute": [
                "any"
            ],
            "events": [],
            "scopes": [
                "users.read"
            ],
            "schedule": "",
            "timeout": 15,
            "enabled": true,
            "logging": true,
            "entrypoint": "src/main.js",
            "commands": "npm install",
            "path": "functions/Adding Attributes"
        },
        {
            "$id": "678f02210010316494f9",
            "name": "Attributes adding",
            "runtime": "node-16.0",
            "execute": [
                "any"
            ],
            "events": [],
            "scopes": [
                "users.read"
            ],
            "schedule": "",
            "timeout": 15,
            "enabled": true,
            "logging": true,
            "entrypoint": "src/main.js",
            "commands": "npm install",
            "path": "functions/Attributes adding"
        },
        {
            "$id": "create-product",
            "name": "Create Product",
            "runtime": "node-16.0",
            "execute": [
                "any"
            ],
            "events": [],
            "scopes": [
                "sessions.write",
                "users.read",
                "users.write",
                "teams.read",
                "teams.write",
                "databases.read",
                "databases.write",
                "collections.read",
                "collections.write",
                "attributes.read",
                "attributes.write",
                "indexes.read",
                "indexes.write",
                "documents.read",
                "documents.write",
                "files.read",
                "files.write",
                "buckets.read",
                "buckets.write",
                "functions.read",
                "functions.write",
                "execution.read",
                "execution.write",
                "targets.read",
                "targets.write",
                "providers.read",
                "providers.write",
                "messages.read",
                "messages.write",
                "topics.read",
                "topics.write",
                "subscribers.read",
                "subscribers.write",
                "locale.read",
                "avatars.read",
                "health.read",
                "migrations.read",
                "migrations.write"
            ],
            "schedule": "",
            "timeout": 15,
            "enabled": true,
            "logging": true,
            "entrypoint": "src/main.js",
            "commands": "npm install",
            "path": "functions/Create Product"
        }
    ]
}